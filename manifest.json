{
    "browser_action": {
        "default_icon": {
            "19": "img/icon.png",
            "38": "img/icon.png"
        }
    },
    "background": {
        "persistent": true,
        "scripts": [ "data/options/config.js", "app.js" ]
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval' https://*.freelancer.com/*; object-src 'self'",
    "icons": {
        "128": "img/icon.png",
        "16": "img/icon.png",
        "48": "img/icon.png"
    },
    "options_page": "data/options/options.html",
    "description": " Bring Freelancer.com New Project Notification to Desktop Notification",
    "homepage_url": "http://autoclick.us/projects",
    "manifest_version": 2,
    "name": "Freelancer Notifier",
    "short_name": "Notifier",
    "permissions": [ "notifications", "storage", "cookies", "tabs", "https://*.freelancer.com/*" ],
    "version": "2015.2.26"
}
